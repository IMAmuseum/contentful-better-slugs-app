{
  "name": "contentful-better-slugs-app",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "Paulo Gomes",
    "email": "pauloamgomes@gmail.com"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "upload": "contentful-app-scripts upload --bundle-dir ./build",
    "upload-ci": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id $CONTENTFUL_ORG_ID --definition-id $CONTENTFUL_APP_DEF_ID --token $CONTENTFUL_ACCESS_TOKEN"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@contentful/app-sdk": "^4.23.1",
    "@contentful/f36-components": "4.56.0",
    "@contentful/f36-icons": "^4.27.0",
    "@contentful/f36-tokens": "4.0.2",
    "@contentful/react-apps-toolkit": "1.2.2",
    "@emotion/css": "^11.11.2",
    "@sindresorhus/slugify": "^2.2.1",
    "contentful": "^10.8.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@contentful/app-scripts": "1.13.2",
    "@testing-library/jest-dom": "6.1.5",
    "@testing-library/react": "14.1.2",
    "@tsconfig/create-react-app": "2.0.1",
    "@types/jest": "29.5.11",
    "@types/node": "20.10.4",
    "@types/react": "18.2.42",
    "@types/react-dom": "18.2.17",
    "cross-env": "7.0.3",
    "esbuild": "^0.19.8",
    "typescript": "4.9.5"
  },
  "homepage": "."
}
